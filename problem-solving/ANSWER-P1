#include<stdio.h>
int main()
{   int ranks;
    printf("how many ranks?\n");
    scanf("%d",&ranks);
    int rank;
    for(int i=1;i<=ranks;i++)
    {
      printf("enter rank: ",i);
      scanf("%d",&rank);
      if(rank<=10)
      {
        printf("YES! \n");
      }
      else{
        printf("NO! \n");
      }
       
    }
    
    return 0;
}
